
<img class="w-full"
    src="{{ image | img_url }}"
    srcset="{{ image | img_url: '180x' }} 180w,
            {{ image | img_url: '320x' }} 320w,
            {{ image | img_url: '480x' }} 480w,
            {{ image | img_url: '640x' }} 640w,
            {{ image | img_url: '768x' }} 768w,
            {{ image | img_url: '1024x' }} 1024w,
            {{ image | img_url: '1280x' }} 1280w,
            {{ image | img_url: '1536x' }} 1536w,
            {{ image | img_url: '1820x' }} 1820w,
            {{ image | img_url: '2048x' }} 2048w"
    alt="{{ image.alt }}"
/>